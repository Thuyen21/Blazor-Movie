<h3>ResetPassword</h3>
@page "/ResetPassword"

<EditForm Model="@resetPasswordModel" OnValidSubmit="@HandleValidSubmit">
    <DataAnnotationsValidator/>
    <ValidationSummary/>
    Email
    <InputText  @bind-Value="@resetPasswordModel.Email"/>
    
    <button type="submit">Submit</button>
</EditForm>

<div>@content</div>



