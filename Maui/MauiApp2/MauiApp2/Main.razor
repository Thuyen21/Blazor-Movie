
<CascadingAuthenticationState>
    <Router AppAssembly="@GetType().Assembly">
	<Found Context="routeData">
		<AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(NavMenu)" />
	</Found>
	<NotFound>
		<LayoutView Layout="@typeof(NavMenu)">
			<p>Sorry, there's nothing at this address.</p>
		</LayoutView>
	</NotFound>
</Router>
</CascadingAuthenticationState>