<h3>StatusStudio</h3>
@attribute [Authorize(Roles ="Studio")]
@page "/StatusStudio/{Id}"

@if(commentStatus.Count < 1)
{
    <div>loadding</div>
}
else
{
    <div>Positive: @commentStatus[0]</div>

<div>Negative: @commentStatus[1]</div>
<button @onclick="Submit">Make AI check again</button>
<div>@content</div>
}


